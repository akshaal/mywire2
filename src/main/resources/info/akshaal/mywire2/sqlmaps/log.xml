<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
          PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
                 "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="info.akshaal.mywire2.log">
    <insert id="insertLogRecord"
            parameterClass="info.akshaal.mywire2.domain.LogRecord">
        INSERT DELAYED INTO log SET time            = #time#,
                                    nano            = #nano#,
                                    log_level_id    = #levelId#,
                                    category        = #category#,
                                    msg             = #msg#,
                                    thread          = #thread#,
                                    throwable       = #throwable#
    </insert>
</sqlMap>
